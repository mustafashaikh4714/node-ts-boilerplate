    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "joi": "^17.2.1",
    "jwt-simple": "^0.5.6",
    "mongoose": "^5.10.3",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "typegoose": "^5.9.1",
    "typescript": "^4.0.3"
  }
