{
  "name": "node-api-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "env-cmd -f dev.env ts-node-dev -r tsconfig-paths/register --notify=false --rs src/server.ts",
    "start": "env-cmd -f dev.env node dist/server.js",
    "build": "tsc -p .",
    "lint": "eslint '*/**/*.{js,ts}'",
    "test": "env-cmd --silent -f test.env jest --no-cache",
    "lint:fix": "eslint '*/**/*.{js,ts}' --quiet --fix"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleDirectories": [
      ".",
      "node_modules",
      "src",
      "utils",
      "controllers",
      "middlewares",
      "models"
    ],
    "setupFilesAfterEnv": [
      "./src/test/test-setup.ts"
    ]
  },
  "author": "mustafa shaikh",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@typegoose/typegoose": "^7.3.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/joi": "^14.3.4",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.2",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.4",
    "husky": "^4.3.0",
    "jest": "^26.4.0",
    "mongodb-memory-server": "^6.9.0",
    "nodemon-webpack-plugin": "^4.3.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3",
    "webpack-node-externals": "^1.7.2"
  },
